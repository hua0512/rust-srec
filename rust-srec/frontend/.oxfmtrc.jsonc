{
  "$schema": "https://raw.githubusercontent.com/oxc-project/oxc/main/npm/oxfmt/configuration_schema.json",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "ignorePatterns": [
    "**/routeTree.gen.ts",
    "src/api/proto/**",
    ".output/**",
    "src/locales/**",
    "dist-desktop/**",
    "dist/**",
    "node_modules/**",
  ],
}